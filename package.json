{
  "name": "@mz3/express-typeorm",
  "version": "0.0.1",
  "dependencies": {
    "pg": "^7.17.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "0.2.22",
    "ts-node-dev": "^1.0.0-pre.44",
    "@types/express": "^4.17.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/ramda": "^0.26.38",
    "@types/webpack-env": "^1.14.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "dotenv-cli": "^3.1.0",
    "execa": "^4.0.0",
    "jest": "^24.8.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.2.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.3"
  },
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev src/index.ts",
    "test": "jest -c ../../jest.unit.js $PWD",
    "build": "tsc -b -v",
    "clean": "tsc -b --clean",
    "prepare": "yarn clean; yarn build",
    "startdb": "bin/psql-db.sh",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/typeorm.config.ts",
    "migration:generate": "yarn typeorm:cli migration:generate -n",
    "db:create": "bin/create-db.sh",
    "db:drop": "bin/drop-db.sh",
    "db:migrate": "yarn typeorm:cli migration:run",
    "db:revert": "yarn typeorm:cli migration:revert",
    "client": "bin/build-client.ts"
  }
}
